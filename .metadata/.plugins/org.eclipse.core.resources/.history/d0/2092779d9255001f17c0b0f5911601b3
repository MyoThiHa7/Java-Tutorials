package Learning;

public class Test4 {
	public static void main(String args[])
	{
		byte num1=7,num2=2;
		System.out.println("Number1 = "+num1);
		System.out.println("Number2 = "+num2);
		System.out.println("Number1 + Number2 = "+(num1+num2));
		System.out.println(num1+ " + " +num2 " = "+ (num1+num2));
	}
}